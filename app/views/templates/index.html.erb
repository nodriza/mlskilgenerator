<p id="notice"><%= notice %></p>

<h1>Fichas skil</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Head</th>
      <th>Body</th>
      <th>Imgurl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @templates.each do |template| %>
      <tr>
        <td><%= template.id %></td>
        <td><%= template.head %></td>
        <td><%= template.body %></td>
        <td><%= template.imgurl %></td>
        <td><%= link_to 'Show', template %></td>
        <td><%= link_to 'Edit', edit_template_path(template) %></td>
        <td><%= link_to 'Destroy', template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Template', new_template_path %>
